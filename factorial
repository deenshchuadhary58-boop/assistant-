public class FactorialLoop {
    public static void main(String[] args) {
        int N = 5; // You can change the value of N for different results
        int factorial = 1;

        for (int i = 1; i <= N; i++) {
            factorial *= i;
        }

        System.out.println("Factorial of " + N + " is: " + factorial);
    }
}
